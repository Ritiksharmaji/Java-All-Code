<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">


    <servlet>
    <servlet-name>SubmitRegistrationServlet</servlet-name>
    <servlet-class>SubmitRegistrationServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>SubmitRegistrationServlet</servlet-name>
    <url-pattern>/submit_registration</url-pattern>
</servlet-mapping>

<filter>
    <filter-name>Multipart Filter</filter-name>
    <filter-class>org.apache.commons.fileupload.servlet.FileUploadFilter</filter-class>
</filter>

</web-app>
